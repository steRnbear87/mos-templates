{
    "name": "Altmount",
    "repo": "ghcr.io/javi11/altmount:latest",
    "registry": "https://ghcr.io/javi11/altmount",
    "network": "bridge",
    "custom_ip": null,
    "default_shell": "bash",
    "privileged": false,
    "extra_parameters": "--restart unless-stopped --cap-add=SYS_ADMIN --device /dev/fuse:/dev/fuse:rwm --security-opt apparmor=unconfined --add-host host.docker.internal:host-gateway",
    "post_parameters": null,
    "cpu_set": null,
    "web_ui_url": "http://[IP]:[PORT:8080]/",
    "icon": "https://github.com/javi11/altmount/blob/main/docs/static/img/logo.png",
    "category": "Storage:Mount Filesystem:Fuse Media",
    "project": "https://github.com/javi11/altmount",
    "support": "https://github.com/javi11/altmount/issues",
    "description": "Altmount is a FUSE-based tool for mounting and exposing remote storage providers inside Docker containers, useful for streaming and media workflows.",
    "donate": "https://paypal.me/ibracorp",
    "requires": null,
    "paths": [
        {
            "name": "Altmount Config",
            "host": "/mnt/cache/altmount/config",
            "container": "/config"
        },
        {
            "name": "Host Mount Root",
            "host": "/mnt/data",
            "container": "/data"
        }
    ],
    "ports": [
        {
            "name": "Web UI",
            "protocol": "tcp",
            "host": "8080",
            "container": "8080"
        }
    ],
    "variables": [
        {
            "name": "User ID",
            "key": "UID",
            "value": "0",
            "mask": false
        },
        {
            "name": "Group ID",
            "key": "GID",
            "value": "0",
            "mask": false
        },
        {
            "name": "Web UI Port",
            "key": "PORT",
            "value": "8080",
            "mask": false
        },
        {
            "name": "Cookie Domain",
            "key": "COOKIE_DOMAIN",
            "value": "localhost",
            "mask": false
        }
    ],
    "devices": [
        {
            "name": "FUSE Device",
            "host": "/dev/fuse",
            "container": "/dev/fuse"
        }
    ],
    "labels": [],
    "gpus": []
}