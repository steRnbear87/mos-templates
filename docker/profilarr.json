{
    "name": "Profilarr",
    "repo": "santiagosayshey/profilarr:latest",
    "registry": "https://hub.docker.com/r/santiagosayshey/profilarr",
    "network": "bridge",
    "custom_ip": null,
    "default_shell": "bash",
    "privileged": false,
    "extra_parameters": "--restart unless-stopped",
    "post_parameters": null,
    "cpu_set": null,
    "web_ui_url": "http://[IP]:[PORT:6868]/",
    "icon": "https://raw.githubusercontent.com/santiagosayshey/profilarr/main/docs/logo.png",
    "category": "MediaServer:Management Automation",
    "project": "https://github.com/santiagosayshey/profilarr",
    "support": "https://github.com/santiagosayshey/profilarr/issues",
    "description": "Profilarr is a companion tool for the ARR ecosystem that helps manage and synchronize quality profiles and settings across Sonarr, Radarr, and related services.",
    "donate": "https://paypal.me/ibracorp",
    "requires": null,
    "paths": [
        {
            "name": "Profilarr Config",
            "host": "/mnt/cache/profilarr/config",
            "container": "/config"
        }
    ],
    "ports": [
        {
            "name": "Web UI",
            "protocol": "tcp",
            "host": "6868",
            "container": "6868"
        }
    ],
    "variables": [
        {
            "name": "User ID",
            "key": "UID",
            "value": "0",
            "mask": false
        },
        {
            "name": "Group ID",
            "key": "GID",
            "value": "0",
            "mask": false
        },
        {
            "name": "Timezone",
            "key": "TZ",
            "value": "America/Chicago",
            "mask": false
        }
    ],
    "devices": [],
    "labels": [],
    "gpus": []
}