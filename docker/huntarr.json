{
  "name": "Huntarr",
  "repo": "github.com/plexguide/huntarr:latest",
  "registry": "https://github.com/plexguide/huntarr",
  "network": "bridge",
  "custom_ip": null,
  "default_shell": "bash",
  "privileged": false,
  "extra_parameters": "--restart unless-stopped",
  "post_parameters": null,
  "cpu_set": null,
  "web_ui_url": "http://127.0.0.1:[PORT:9705]/",
  "icon": "https://cdn.jsdelivr.net/gh/selfhst/icons@main/png/huntarr.png",
  "category": "MediaServer:Management Automation",
  "project": "https://github.com/plexguide/Huntarr",
  "support": "https://github.com/plexguide/Huntarr/issues",
  "description": "Huntarr is a lightweight ARR stack manager that orchestrates Radarr and Sonarr tasks, providing a simple web interface for monitoring and managing media services.",
  "donate": "https://paypal.me/ibracorp",
  "requires": null,
  "paths": [
    {
      "name": "Huntarr Config",
      "host": "/mnt/cache/huntarr/config",
      "container": "/config"
    },
    {
      "name": "Localtime",
      "host": "/etc/localtime",
      "container": "/etc/localtime"
    }
  ],
  "ports": [
    {
      "name": "Web UI",
      "protocol": "tcp",
      "host": "9705",
      "container": "9705"
    }
  ],
  "variables": [],
  "devices": [],
  "labels": [],
  "gpus": []
}
