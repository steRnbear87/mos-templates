{
    "name": "Agregarr",
    "repo": "agregarr/agregarr:latest",
    "registry": "https://hub.docker.com/r/agregarr/agregarr",
    "network": "bridge",
    "custom_ip": null,
    "default_shell": "bash",
    "privileged": false,
    "extra_parameters": "--restart unless-stopped",
    "post_parameters": null,
    "cpu_set": null,
    "web_ui_url": "http://[IP]:[PORT:7171]/",
    "icon": "https://cdn.jsdelivr.net/gh/selfhst/icons@main/svg/agregarr.svg",
    "category": "MediaServer:Management Automation",
    "project": "https://github.com/agregarr/agregarr",
    "support": "https://github.com/agregarr/agregarr/issues",
    "description": "Agregarr aggregates and centralizes media-related services (Sonarr, Radarr, Lidarr, etc.) into a single interface to simplify management of ARRs-based media stacks.",
    "donate": "https://paypal.me/ibracorp",
    "requires": null,
    "paths": [
        {
            "name": "Agregarr Config",
            "host": "/mnt/cache/agregarr/config",
            "container": "/config"
        },
        {
            "name": "Media (Altmount)",
            "host": "/mnt/data/media",
            "container": "/mnt/data/media"
        }
    ],
    "ports": [
        {
            "name": "Web UI",
            "protocol": "tcp",
            "host": "7171",
            "container": "7171"
        }
    ],
    "variables": [
        {
            "name": "User ID",
            "key": "UID",
            "value": "0",
            "mask": false
        },
        {
            "name": "Group ID",
            "key": "GID",
            "value": "0",
            "mask": false
        },
        {
            "name": "Timezone",
            "key": "TZ",
            "value": "America/Chicago",
            "mask": false
        }
    ],
    "devices": [],
    "labels": [],
    "gpus": []
}