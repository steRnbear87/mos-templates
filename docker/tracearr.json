{
    "name": "Tracearr",
    "repo": "ghcr.io/connorgallopo/tracearr:supervised",
    "registry": "https://github.com/connorgallopo/tracearr/pkgs/container/tracearr",
    "network": "bridge",
    "custom_ip": null,
    "default_shell": "bash",
    "privileged": false,
    "extra_parameters": "--restart unless-stopped --shm-size=512mb --memory=3g --ulimit nofile=65536:65536",
    "post_parameters": null,
    "cpu_set": null,
    "web_ui_url": "http://[IP]:[PORT:3005]/",
    "icon": "https://raw.githubusercontent.com/connorgallopo/tracearr/main/apps/web/public/favicon.ico",
    "category": "MediaServer:Monitoring Observability",
    "project": "https://github.com/connorgallopo/tracearr",
    "support": "https://github.com/connorgallopo/tracearr/issues",
    "description": "Tracearr provides observability and monitoring for ARR media automation stacks, offering tracing, metrics, and logs to help diagnose performance and workflow issues across Sonarr, Radarr, and related services.",
    "donate": "https://paypal.me/ibracorp",
    "requires": null,
    "paths": [
        {
            "name": "Postgres Data",
            "host": "/mnt/cache/tracearr/postgres",
            "container": "/data/postgres"
        },
        {
            "name": "Redis Data",
            "host": "/mnt/cache/tracearr/redis",
            "container": "/data/redis"
        },
        {
            "name": "Tracearr Data",
            "host": "/mnt/cache/tracearr/data",
            "container": "/data/tracearr"
        }
    ],
    "ports": [
        {
            "name": "Web UI",
            "protocol": "tcp",
            "host": "3005",
            "container": "3000"
        }
    ],
    "variables": [
        {
            "name": "User ID",
            "key": "UID",
            "value": "0",
            "mask": false
        },
        {
            "name": "Group ID",
            "key": "GID",
            "value": "0",
            "mask": false
        },
        {
            "name": "Timezone",
            "key": "TZ",
            "value": "America/Chicago",
            "mask": false
        },
        {
            "name": "Log Level",
            "key": "LOG_LEVEL",
            "value": "info",
            "mask": false
        }
    ],
    "devices": [],
    "labels": [],
    "gpus": []
}